{
    "name": "wearehaus/site-widgets",
    "description": "Site specific widgets",
    "keywords": ["wordpress", "plugin"],
    "homepage": "https://plugins.webien.io",
    "license": "MIT",
    "authors": [
      {
      "name": "Webien",
      "email": "service@haus.se",
      "homepage": "https://plugins.webien.io"
      }
    ],
    "type": "wordpress-plugin",
    "autoload"   : {
      "psr-4": {
        "Webien\\Site\\": "src"
      }
    },
    "require"    : {
      "php": ">=8.1",
      "composer/installers": "~1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    }
}
